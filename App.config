<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

    <connectionStrings>
        <add name="DbAddr"
            connectionString="Server=localhost\sqlexpress;Database=PersianSwitch;Trusted_Connection=True;"
            providerName="System.Data.SqlClient"/>
    </connectionStrings>

  <appSettings file="">
    <clear />
    <add key="serverIp" value="localhost" />
    <add key="serverPort" value="2001" />
  </appSettings>
</configuration>